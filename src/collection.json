{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "module": {
      "description": "Generate module and update router module",
      "factory": "./module/index",
      "schema": "./module/schema.json",
      "aliases": ["m"]
    },
    "ng-add": {
      "description": "Add library to Angular project dependencies and install it.",
      "factory": "./ng-add/index#ngAdd"
    },
    "guard": {
      "description": "Add guards to Angular project",
      "factory": "./guard/index#guard",
      "schema": "./guard/schema.json"
    },
    "service": {
      "description": "Add services to Angular project",
      "factory": "./service/index#service",
      "schema": "./service/schema.json"
    },
    "modules": {
      "description": "Add list of modules to Angular project",
      "factory": "./modules/index#modules",
      "schema": "./modules/schema.json"
    }
  }
}
